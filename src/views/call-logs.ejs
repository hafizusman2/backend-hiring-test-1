<!DOCTYPE html>
<html>
<head>
  <title>Call Logs</title>
  <style>
    table {
      width: 100%;
      border-collapse: collapse;
    }
    th, td {
      padding: 8px;
      text-align: left;
      border-bottom: 1px solid #ddd;
    }
    th {
      background-color: #f2f2f2;
    }
  </style>
</head>
<body>
  <h1>Call Logs</h1>
  <table>
    <thead>
      <tr>
        <th>ID</th>
        <th>SID</th>
        <th>Type</th>
        <th>Status</th>
        <th>To</th>
        <th>From</th>
        <th>Duration (s)</th>
        <th>Voicemail URL</th>
        <th>Created At</th>
        <th>Updated At</th>
      </tr>
    </thead>
    <tbody>
      <% calls.forEach(call => { %>
        <tr>
          <td><%= call.id %></td>
          <td><%= call.sid %></td>
          <td><%= call.type %></td>
          <td><%= call.status %></td>
          <td><%= call.to %></td>
          <td><%= call.from %></td>
          <td><%= call.duration %></td>
          <td>
            <% if (call.voicemailUrl) { %>
              <a href="<%= call.voicemailUrl %>">Voicemail</a>
            <% } else { %>
              N/A
            <% } %>
          </td>
          <td><%= call.createdAt.toISOString() %></td>
          <td><%= call.updatedAt.toISOString() %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>